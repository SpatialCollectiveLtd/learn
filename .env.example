PGPASSWORD=your-password

# Vercel Postgres Environment Variables
POSTGRES_URL=postgresql://username:password@your-neon-host.neon.tech/database?sslmode=require
POSTGRES_URL_NON_POOLING=postgresql://username:password@your-neon-host.neon.tech/database?sslmode=require
POSTGRES_USER=neondb_owner
POSTGRES_HOST=your-neon-host.neon.tech
POSTGRES_PASSWORD=your-password
POSTGRES_DATABASE=neondb
POSTGRES_URL_NO_SSL=postgresql://username:password@your-neon-host.neon.tech/database
POSTGRES_PRISMA_URL=postgresql://username:password@your-neon-host.neon.tech/database?connect_timeout=15&sslmode=require

# Neon Auth Environment Variables (Optional)
